# .coveragerc to control coverage.py
[run]
source = wqio
branch = True
include = wqio/*.py
omit =
    wqio/tests/*

[report]
include = wqio/*.py
ignore_errors = True
omit =
    wqio/tests/*
    wqio/testutils/*

# Regexes for lines to exclude from consideration
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
